!
!

!--------------------------------------------------------------------
!
!   URxvt
!
!--------------------------------------------------------------------

URxvt*saveline: 15000
URxvt*termName: rxvt-256color
URxvt*iso14755: false
URxvt*depth: 32
URxvt*background: [70]#000000
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*internalBorder: 20
URxvt*externalBorder: 0
URxvt*letterSpace: 0
!URxvt.font: xft:DejaVu Sans Mono:autohint=true:size=9,xft:Inconsolata\ for\ Powerline:autohint=true:size=9
!URxvt.font: 9x15,xft:TerminessTTFNerdFontMono
!URxvt.font: Monospace 13
!URxvt.font: 9x15,xft:TerminessTTFNerdFontMono, Powerline:autohint=true:size=9
URxvt.font: xft:Source\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=true\ , Powerline:size=12
!urxvt*font: xft:Terminus:pixelsize=12,xft:Inconsolata\ for\ Powerline:pixelsize=12
!URxvt*font: xft:Terminess Powerline:size=12

!URxvt*font: DejaVu Sans Mono Book:size=12:antialias=false

URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.keysym.m-c: perl:clipboard:copy
URxvt.keysym.m-v: perl:clipboard:paste
URxvt.urlLauncher: exo-open
URxvt.underlineURLs: true
URxvt.urlButton: 1
URxvt.geometry: 90x30
URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 16
URxvt.tabbed.tab-fg: 15
URxvt.tabbed.tab-bg: 4
URxvt*buffered: false
URxvt.cursorColor:  #31e62b 

!-------------------------------------------------------------------
!
!   ST
!
!--------------------------------------------------------------------
St.font: Liberation Mono:pixelsize=13:antialias=true:autohint=true;
!St.font: Ubuntu Mono Nerd Font:pixelsize=15;
st.internalBorder: 20px
*.alpha: 0.70
st.borderpx: 5
